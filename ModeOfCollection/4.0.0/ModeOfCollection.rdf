      <skos:notation xml:lang="en">ModeOfCollection</skos:notation>
      <dcterms:title xml:lang="en">Mode Of Collection</dcterms:title>
      <dcterms:description xml:lang="en">The procedure, technique, or mode of inquiry used to attain the data.</dcterms:description>
      <skos:prefLabel xml:lang="en">Mode Of Collection</skos:prefLabel>
      <dcterms:title xml:lang="de">Erhebungsmodus</dcterms:title>
      <dcterms:description xml:lang="de">Prozedur, Technik oder Art der Erhebung, die verwendet wurde, um Daten zu bekommen.</dcterms:description>
      <skos:prefLabel xml:lang="de">Erhebungsmodus</skos:prefLabel>
      <dcterms:title xml:lang="fi">Keruumenetelm&#xe4;</dcterms:title>
      <dcterms:description xml:lang="fi">Kuvailtavan aineiston ensisijainen keruumenetelm&#xe4;.</dcterms:description>
      <skos:prefLabel xml:lang="fi">Keruumenetelm&#xe4;</skos:prefLabel>
      <dcterms:title xml:lang="it">Metodo di raccolta dei dati</dcterms:title>
      <dcterms:description xml:lang="it">La procedura, la tecnica o la modalit&#xe0; di indagine utilizzata per ottenere i dati.</dcterms:description>
      <skos:prefLabel xml:lang="it">Metodo di raccolta dei dati</skos:prefLabel>
      <dcterms:title xml:lang="ja">&#x8abf;&#x67fb;&#x65b9;&#x6cd5;</dcterms:title>
      <dcterms:description xml:lang="ja">&#x30c7;&#x30fc;&#x30bf;&#x3092;&#x53d6;&#x5f97;&#x3059;&#x308b;&#x306b;&#x3042;&#x305f;&#x308a;&#x5229;&#x7528;&#x3055;&#x308c;&#x305f;&#x8abf;&#x67fb;&#x306e;&#x624b;&#x7d9a;&#x304d;&#x3001;&#x6280;&#x8853;&#x3042;&#x308b;&#x3044;&#x306f;&#x65b9;&#x6cd5;&#x3002;</dcterms:description>
      <skos:prefLabel xml:lang="ja">&#x8abf;&#x67fb;&#x65b9;&#x6cd5;</skos:prefLabel>
      <dcterms:title xml:lang="lt">Surinkimo b&#x16b;das</dcterms:title>
      <dcterms:description xml:lang="lt">Tyrimo proced&#x16b;ra, technika ar b&#x16b;das, naudojami gauti duomenis.</dcterms:description>
      <skos:prefLabel xml:lang="lt">Surinkimo b&#x16b;das</skos:prefLabel>
      <dcterms:title xml:lang="nl">Verzamelmethode</dcterms:title>
      <dcterms:description xml:lang="nl">De procedure, techniek of methode die is gebruikt om de gegevens te verzamelen. </dcterms:description>
      <skos:prefLabel xml:lang="nl">Verzamelmethode</skos:prefLabel>
      <dcterms:title xml:lang="no">Innsamlingsmetode</dcterms:title>
      <dcterms:description xml:lang="no">Framgangsm&#xe5;ten, teknikken, eller metoden som er anvendt for &#xe5; samle inn data.</dcterms:description>
      <skos:prefLabel xml:lang="no">Innsamlingsmetode</skos:prefLabel>
      <dcterms:title xml:lang="pt">M&#xe9;todo de recolha de dados</dcterms:title>
      <dcterms:description xml:lang="pt">Procedimentos, t&#xe9;cnicas ou m&#xe9;todos de recolha de dados.</dcterms:description>
      <skos:prefLabel xml:lang="pt">M&#xe9;todo de recolha de dados</skos:prefLabel>
      <dcterms:title xml:lang="sl">Na&#x10d;in zbiranja podatkov</dcterms:title>
      <dcterms:description xml:lang="sl">Postopki, tehnike ali na&#x10d;ini raziskovanja, uporabljeni za pridobivanje podatkov.</dcterms:description>
      <skos:prefLabel xml:lang="sl">Na&#x10d;in zbiranja podatkov</skos:prefLabel>
      <dcterms:title xml:lang="sv">Insamlingsmetod</dcterms:title>
      <dcterms:description xml:lang="sv">Den process, metod eller teknik som anv&#xe4;ndes f&#xf6;r att samla in data.</dcterms:description>
      <skos:prefLabel xml:lang="sv">Insamlingsmetod</skos:prefLabel>
