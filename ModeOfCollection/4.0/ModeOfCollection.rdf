      <skos:narrower rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/ModeOfCollection/4.0/d50e420"/>
      <skos:broader rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/ModeOfCollection/4.0/d50e136"/>
