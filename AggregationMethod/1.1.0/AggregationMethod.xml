         <r:UserID typeOfUserID="URI">http://rdf-vocabulary.ddialliance.org/cv/AggregationMethod/1.1.2/66851a3</r:UserID>
         <l:CategoryName>Median</l:CategoryName>
         <r:Label>
            <r:Content xml:lang="en">Median</r:Content>
            <r:Content xml:lang="no">Median</r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">The middle value in a series of values arranged sequentially from smallest to largest.</r:Content>
            <r:Content xml:lang="no">Midtverdien i en serie av verdier som er arrangert i rekkefølge fra minst til størst.</r:Content>
         </r:Description>
      </l:Category>
   </ddi:Fragment>
   <ddi:Fragment>
      <l:Category>
         <r:Agency>int.ddi.cv</r:Agency>
         <r:ID>AggregationMethod-8321e79-Category</r:ID>
         <r:Version>1.1.2</r:Version>
         <r:UserID typeOfUserID="URI">http://rdf-vocabulary.ddialliance.org/cv/AggregationMethod/1.1.2/8321e79</r:UserID>
         <l:CategoryName>Maximum</l:CategoryName>
         <r:Label>
            <r:Content xml:lang="en">Maximum</r:Content>
            <r:Content xml:lang="no">Maksimum</r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">The highest value attained or recorded.</r:Content>
            <r:Content xml:lang="no">Den høyeste oppnådde eller registrerte verdi.</r:Content>
         </r:Description>
      </l:Category>
   </ddi:Fragment>
   <ddi:Fragment>
      <l:Category>
         <r:Agency>int.ddi.cv</r:Agency>
         <r:ID>AggregationMethod-a1d0ec6-Category</r:ID>
         <r:Version>1.1.2</r:Version>
