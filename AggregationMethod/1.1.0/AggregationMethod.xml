         <l:CategoryName>Mode</l:CategoryName>
         <r:Label>
            <r:Content xml:lang="en">Mode</r:Content>
            <r:Content xml:lang="no">Modus</r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">The value that appears most often in a set of values.</r:Content>
            <r:Content xml:lang="no">Den verdien som forekommer mest i et sett av verdier.</r:Content>
         </r:Description>
      </l:Category>
   </ddi:Fragment>
   <ddi:Fragment>
      <l:Category>
         <r:Agency>int.ddi.cv</r:Agency>
