         <r:UserID typeOfUserID="URI">http://rdf-vocabulary.ddialliance.org/cv/AggregationMethod/1.1.2/6311ae1</r:UserID>
         <l:CategoryName>Other</l:CategoryName>
         <r:Label>
            <r:Content xml:lang="en">Other</r:Content>
            <r:Content xml:lang="no">Annet</r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">Use when the aggregation method is known, but not found in the list.</r:Content>
            <r:Content xml:lang="no">Bruk når aggregeringsmetoden er kjent, men ikke er på denne listen.</r:Content>
         </r:Description>
