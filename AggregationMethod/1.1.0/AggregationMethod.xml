         <l:CategoryName>Count</l:CategoryName>
         <r:Label>
            <r:Content xml:lang="en">Count</r:Content>
            <r:Content xml:lang="no">Antall</r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">Total number of units in a group of items.</r:Content>
            <r:Content xml:lang="no">Samlet antall enhenter i en gruppe av elementer</r:Content>
         </r:Description>
      </l:Category>
   </ddi:Fragment>
   <ddi:Fragment>
      <l:Category>
         <r:Agency>int.ddi.cv</r:Agency>
