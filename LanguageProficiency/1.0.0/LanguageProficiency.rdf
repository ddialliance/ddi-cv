   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/LanguageProficiency/1.0.0/">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <dcterms:isVersionOf rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/LanguageProficiency/"/>
      <skos:notation xml:lang="en">LanguageProficiency</skos:notation>
      <dcterms:title xml:lang="en">Language Proficiency</dcterms:title>
      <dcterms:description xml:lang="en">Describes the level of proficiency of an individual in a natural language.</dcterms:description>
      <skos:prefLabel xml:lang="en">Language Proficiency</skos:prefLabel>
      <dcterms:title xml:lang="no">Spr&#xe5;kferdigheter</dcterms:title>
      <dcterms:description xml:lang="no">Beskriver ferdighetene et individ har i et naturlig spr&#xe5;k.</dcterms:description>
      <skos:prefLabel xml:lang="no">Spr&#xe5;kferdigheter</skos:prefLabel>
      <owl:versionInfo>1.0.0</owl:versionInfo>
