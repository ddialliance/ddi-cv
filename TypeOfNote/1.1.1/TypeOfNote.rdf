   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/643562a">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/"/>
      <skos:notation xml:lang="en">Processing</skos:notation>
      <skos:prefLabel xml:lang="lt">Apdorojimo pastaba</skos:prefLabel>
      <skos:definition xml:lang="lt">Pastaba, susijusi su duomen&#x173; ir (arba) metaduomen&#x173; tvarkymu.</skos:definition>
      <skos:prefLabel xml:lang="en">Processing note</skos:prefLabel>
      <skos:definition xml:lang="en">A note related to the processing of the data, or metadata, or both.</skos:definition>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/efcfc2f">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/"/>
      <skos:notation xml:lang="en">Addendum</skos:notation>
      <skos:prefLabel xml:lang="lt">Papildymas</skos:prefLabel>
      <skos:definition xml:lang="lt">Papildoma informacija, paprastai faktinio pob&#x16b;d&#x17e;io (prie&#x161;ingai nei komentaras), skirta paai&#x161;kinti DDI skyriaus ar modulio turin&#x12f;.</skos:definition>
      <skos:prefLabel xml:lang="en">Addendum</skos:prefLabel>
      <skos:definition xml:lang="en">Additional information, usually of a factual nature (as opposed to a comment), intended to clarify the content of a DDI section or module.</skos:definition>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/a45da96">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/"/>
      <skos:notation xml:lang="en">System</skos:notation>
      <skos:prefLabel xml:lang="lt">Sistemos pastaba</skos:prefLabel>
      <skos:definition xml:lang="lt">Sistemos, skirtos duomen&#x173; ir (arba) metaduomen&#x173; k&#x16b;rimui, sukurta pastaba.</skos:definition>
      <skos:prefLabel xml:lang="en">System note</skos:prefLabel>
      <skos:definition xml:lang="en">A note generated by the processing system used to produce the data, or metadata, or both.</skos:definition>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/c680d43">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/"/>
      <skos:notation xml:lang="en">Observation</skos:notation>
      <skos:prefLabel xml:lang="lt">Pasteb&#x117;jimas</skos:prefLabel>
      <skos:definition xml:lang="lt">Pastaba, kurioje pateikiama informacija apie duomen&#x173; naudojimui svarb&#x173; aspekt&#x105;, grei&#x10d;iausiai b&#x16b;sianti aktuali visada, arba laikina pastaba, kurioje pateikiama informacija yra aktuali tolesnei per&#x17e;i&#x16b;rai ir taisymui. Tai gali b&#x16b;ti &#x17e;mogaus &#x12f;vestas pasteb&#x117;jimas arba sistemos sukurtas pasteb&#x117;jimas apie bet kokius neatitikimus ar nukrypimus.</skos:definition>
      <skos:prefLabel xml:lang="en">Observation</skos:prefLabel>
      <skos:definition xml:lang="en">A note providing information on an aspect relevant for data usage that is likely to remain permanent, or a temporary note providing information for further review and correction. May be an observation entered by a human user or a system-generated note on any inconsistencies or deviations.</skos:definition>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/0be8406">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/"/>
      <skos:notation xml:lang="en">Comment</skos:notation>
      <skos:prefLabel xml:lang="lt">Komentaras</skos:prefLabel>
      <skos:definition xml:lang="lt">&#x17d;mogui suprantamas komentaras, paprastai subjektyvesnio pob&#x16b;d&#x17e;io nei papildymas.</skos:definition>
      <skos:prefLabel xml:lang="en">Comment</skos:prefLabel>
      <skos:definition xml:lang="en">A human-readable comment, typically of a more subjective nature than an addendum.</skos:definition>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/6311ae1">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.1.1/"/>
      <skos:notation xml:lang="en">Other</skos:notation>
      <skos:prefLabel xml:lang="lt">Kita</skos:prefLabel>
      <skos:definition xml:lang="lt">Naudoti, kai pastabos tipas yra &#x17e;inomas, bet s&#x105;ra&#x161;e jo n&#x117;ra.</skos:definition>
      <skos:prefLabel xml:lang="en">Other</skos:prefLabel>
      <skos:definition xml:lang="en">Use when the type of note is known, but not found in the list.</skos:definition>
   </rdf:Description>
</rdf:RDF>
