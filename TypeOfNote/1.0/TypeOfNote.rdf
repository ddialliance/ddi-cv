   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote/1.0/">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <dcterms:isVersionOf rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/TypeOfNote"/>
      <skos:notation>TypeOfNote</skos:notation>
      <skos:prefLabel xml:lang="en">Type of Note</skos:prefLabel>
      <skos:definition xml:lang="en">Includes a typology of notes.</skos:definition>
