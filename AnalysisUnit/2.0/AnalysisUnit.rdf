   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e986">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/"/>
      <skos:notation>MediaUnit.Sound</skos:notation>
      <skos:prefLabel xml:lang="en">Media unit: Sound</skos:prefLabel>
      <skos:definition xml:lang="en">Recorded sound, including voice, music, etc.</skos:definition>
      <skos:prefLabel xml:lang="da">Medieenhed: Lyd</skos:prefLabel>
      <skos:definition xml:lang="da">Optaget lyd, inklusiv stemme, musik etc.</skos:definition>
      <skos:prefLabel xml:lang="fi">Mediatyyppi: ääni</skos:prefLabel>
      <skos:definition xml:lang="fi">Taltioitu ääni, esimerkiksi puhe, musiikki jne.</skos:definition>
      <skos:prefLabel xml:lang="fi">Mediatyyppi: ääni</skos:prefLabel>
      <skos:definition xml:lang="fi">Taltioitu ääni, esimerkiksi puhe, musiikki jne.</skos:definition>
      <skos:prefLabel xml:lang="fr">Média : son</skos:prefLabel>
      <skos:definition xml:lang="fr">Son enregistré, incluant la voix, la musique, etc.</skos:definition>
      <skos:prefLabel xml:lang="fr">Média : son</skos:prefLabel>
      <skos:definition xml:lang="fr">Son enregistré, incluant la voix, la musique, etc.</skos:definition>
      <skos:prefLabel xml:lang="it">Unità di supporto: suono</skos:prefLabel>
      <skos:definition xml:lang="it">Suoni registrati, inclusa la voce, la musica, ecc.</skos:definition>
      <skos:prefLabel xml:lang="no">Medieenhet: Lyd</skos:prefLabel>
      <skos:definition xml:lang="no">Lydopptak, inkludert stemme, musikk, etc.</skos:definition>
      <skos:prefLabel xml:lang="pt">Unidade de Media: Som</skos:prefLabel>
      <skos:definition xml:lang="pt">Som gravado, incluindo voz, música, etc.</skos:definition>
      <skos:prefLabel xml:lang="sr">Medijski zapis: Zvuk</skos:prefLabel>
      <skos:definition xml:lang="sr">Zvučni zapis, uključujući zvuk, muziku i sl.</skos:definition>
      <skos:prefLabel xml:lang="sv">Mediaenhet: Ljud</skos:prefLabel>
      <skos:definition xml:lang="sv">Inspelat ljud, som röst, musik m.m.</skos:definition>
      <skos:broader rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e900"/>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e1064">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/"/>
      <skos:notation>MediaUnit.StillImage</skos:notation>
      <skos:prefLabel xml:lang="en">Media unit: Still image</skos:prefLabel>
      <skos:definition xml:lang="en">Static images, such as graphs, drawings, photographs, diagnostic/medical images like X-rays and other types of imaging tests, etc.</skos:definition>
      <skos:prefLabel xml:lang="da">Medieenhed: Stillbillede</skos:prefLabel>
      <skos:definition xml:lang="da">Statistiske billeder, såsom grafer, tegninger, fotografier, diagnostiske/medicinske billeder som røntgen og andre typer af billeddannelsestests etc.</skos:definition>
      <skos:prefLabel xml:lang="fi">Mediatyyppi: still-kuva</skos:prefLabel>
      <skos:definition xml:lang="fi">Still-kuvat kuten esimerkiksi graafit, piirrokset, valokuvat, röntgenkuvat tai muut lääketieteelliset still-kuvat.</skos:definition>
      <skos:prefLabel xml:lang="fi">Mediatyyppi: still-kuva</skos:prefLabel>
      <skos:definition xml:lang="fi">Still-kuvat kuten esimerkiksi graafit, piirrokset, valokuvat, röntgenkuvat tai muut lääketieteelliset still-kuvat.</skos:definition>
      <skos:prefLabel xml:lang="fr">Média : image fixe</skos:prefLabel>
      <skos:definition xml:lang="fr">Image statique, tel que des graphiques, des dessins, des photographies, des images de diagnostic / médicales comme des rayons X et tous autres types de tests d'imageries, etc.</skos:definition>
      <skos:prefLabel xml:lang="fr">Média : image fixe</skos:prefLabel>
      <skos:definition xml:lang="fr">Image statique, tel que des graphiques, des dessins, des photographies, des images de diagnostic / médicales comme des rayons X et tous autres types de tests d'imageries, etc.</skos:definition>
      <skos:prefLabel xml:lang="it">Unità di supporto: immagine</skos:prefLabel>
      <skos:definition xml:lang="it">Immagini statiche, come grafici, disegni, fotografie, immagini diagnostiche/mediche come radiografie e altri tipi di esami per immagini, ecc.</skos:definition>
      <skos:prefLabel xml:lang="no">Medieenhet: Stillbilde</skos:prefLabel>
      <skos:definition xml:lang="no">Statiske bilder som grafer, tegninger, fotografier, diagnostiske/medisinske bilder som røntgen og andre bildetester, etc.</skos:definition>
      <skos:prefLabel xml:lang="pt">Unidade de Media: Imagem</skos:prefLabel>
      <skos:definition xml:lang="pt">Imagens estáticas, como gráficos, desenhos, fotografias, imagens médicas/de diagnóstico como raio-x e outros tipos de testes de imagem, etc.</skos:definition>
      <skos:prefLabel xml:lang="sr">Medijski zapis: Nepokretna slika</skos:prefLabel>
      <skos:definition xml:lang="sr">Nepokretne slike, kao što su grafici, crteži, fotografije, dijagnostičke/medicinske slike kao što su rentgenski snimci i sl.</skos:definition>
      <skos:prefLabel xml:lang="sv">Mediaenhet: Stillbild</skos:prefLabel>
      <skos:definition xml:lang="sv">Statiska bilder, som diagram, ritningar, fotografier, diagnostiska/medicinska bilder som röntgenbilder eller andra typer av bilddiagnostiska undersökningar&amp;nbsp;osv.</skos:definition>
      <skos:broader rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e900"/>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e1142">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/"/>
      <skos:notation>MediaUnit.Text</skos:notation>
      <skos:prefLabel xml:lang="en">Media unit: Text</skos:prefLabel>
      <skos:definition xml:lang="en">Any written piece/entity, such as books, articles, tweets, blogs, etc. For example, a study on hate speech in tweets, or a study of the way newspapers present certain political/social issues over time, like women's or minorities' rights, etc.</skos:definition>
      <skos:prefLabel xml:lang="da">Medieenhed: Tekst</skos:prefLabel>
      <skos:definition xml:lang="da">Ethvert skriftligt stykke/enhed, såsom bøger, artikler, tweets, blogs etc. Fx en undersøgelse af hadudtalelser i tweets eller en undersøgelse af den måde, aviser præsenterer bestemte politiske/sociale spørgsmål over tid, som kvinders eller mindretals rettigheder etc.</skos:definition>
      <skos:prefLabel xml:lang="fi">Mediatyyppi: teksti</skos:prefLabel>
      <skos:definition xml:lang="fi">Kirja, artikkeli, tweet, blogikirjoitus tai mikä tahansa muu tekstiyksikkö. Esimerkiksi tutkittaessa miten sanomalehdet ovat käsitelleet tiettyä aihetta ajan saatossa tai tutkittaessa vihapuhetta blogikirjoituksissa.</skos:definition>
      <skos:prefLabel xml:lang="fi">Mediatyyppi: teksti</skos:prefLabel>
      <skos:definition xml:lang="fi">Kirja, artikkeli, tweet, blogikirjoitus tai mikä tahansa muu tekstiyksikkö. Esimerkiksi tutkittaessa miten sanomalehdet ovat käsitelleet tiettyä aihetta ajan saatossa tai tutkittaessa vihapuhetta blogikirjoituksissa.</skos:definition>
      <skos:prefLabel xml:lang="fr">Média : texte</skos:prefLabel>
      <skos:definition xml:lang="fr">Toutes pièce / entité écrite, telles que des livres, des articles, des tweets, des blogs, etc. Par exemple, une étude sur le discours de haine dans des tweets, ou une étude sur la manière dont les journaux présentent certaines questions politiques / sociales au fil du temps, comme les droits des femmes ou des minorités, etc.</skos:definition>
      <skos:prefLabel xml:lang="fr">Média : texte</skos:prefLabel>
      <skos:definition xml:lang="fr">Toutes pièce / entité écrite, telles que des livres, des articles, des tweets, des blogs, etc. Par exemple, une étude sur le discours de haine dans des tweets, ou une étude sur la manière dont les journaux présentent certaines questions politiques / sociales au fil du temps, comme les droits des femmes ou des minorités, etc.</skos:definition>
      <skos:prefLabel xml:lang="it">Unità di supporto: testo</skos:prefLabel>
      <skos:definition xml:lang="it">Qualsiasi entità scritta, come libri, articoli, tweet, blog, ecc. Ad esempio, uno studio sugli hate speech nei tweet, oppure uno studio sul modo in cui i giornali presentano nel tempo alcuni temi politici/sociali, come i diritti delle donne o delle minoranze, ecc.</skos:definition>
      <skos:prefLabel xml:lang="no">Medieenhet: Tekst</skos:prefLabel>
      <skos:definition xml:lang="no">Alle skrevne tekster/enheter som bøker, artikler, tweets, blogger, etc. For eksempel: en studie om hatefulle ytringer i tweets, en studie om måten nyhetsaviser fremstiller enkelte politiske/sosiale problemer over tid, som kvinners eller minoriteters rettigheter.</skos:definition>
      <skos:prefLabel xml:lang="pt">Unidade de Media: Texto</skos:prefLabel>
      <skos:definition xml:lang="pt">Qualquer entidade/peça escrita, como livros, artigos, tweets, blogues, etc. Por exemplo, um estudo sobre discurso de ódio nos tweets, ou um estudo sobre a forma como os jornais apresentam certos fenómenos políticos/sociais ao longo do tempo, como os direitos das mulheres ou de outras minorias, etc.</skos:definition>
      <skos:prefLabel xml:lang="sr">Medijski zapis: Tekst</skos:prefLabel>
      <skos:definition xml:lang="sr">Bilo koje pisano delo, kao što su knjige, članci, tvitovi, blogovi, itd. Na primer, studija o govoru mržnje u tvitovima ili studija načina na koji novine predstavljaju određena politička / društvena pitanja tokom vremena, kao što su prava žena ili manjina, itd.</skos:definition>
      <skos:prefLabel xml:lang="sv">Mediaenhet: Text</skos:prefLabel>
      <skos:definition xml:lang="sv">Alla skrivna texter, som böcker, artiklar, tweets, bloggar m.m. Till exempel en studie om hets mot folkgrupp i tweets, eller en studie om hur tidningar beskriver olika politiska/sociala ämnen över tid, som kvinnors eller minoriteters rättigheter osv.</skos:definition>
      <skos:broader rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e900"/>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e1220">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/"/>
      <skos:notation>MediaUnit.Video</skos:notation>
      <skos:prefLabel xml:lang="en">Media unit: Video</skos:prefLabel>
      <skos:definition xml:lang="en">Moving images. May include films, animation, digital recordings, visual output from simulations, recorded television programs, diagnostic/medical videos like ultrasound scans, etc. May be mute or may include synchronized sound.</skos:definition>
      <skos:prefLabel xml:lang="da">Medieenhed: Video</skos:prefLabel>
      <skos:definition xml:lang="da">Bevægende billeder. Kan indeholde film, animation, digitale optagelser, visuelt output fra simuleringer, indspillede tv-programmer, diagnostiske/medicinske videoer som ultralydsscanninger etc. Kan være lydløs eller kan indeholde synkroniseret lyd.</skos:definition>
      <skos:prefLabel xml:lang="fi">Mediatyyppi: liikkuva kuva</skos:prefLabel>
      <skos:definition xml:lang="fi">Liikkuva kuva äänellä tai ilman. Esimerkiksi elokuvat, animaatiot, digitaaliset taltiot, simulaatioiden liikkuvat kuvat, taltioidut televisio-ohjelmat ja ultraäänikuvat.</skos:definition>
      <skos:prefLabel xml:lang="fi">Mediatyyppi: liikkuva kuva</skos:prefLabel>
      <skos:definition xml:lang="fi">Liikkuva kuva äänellä tai ilman. Esimerkiksi elokuvat, animaatiot, digitaaliset taltiot, simulaatioiden liikkuvat kuvat, taltioidut televisio-ohjelmat ja ultraäänikuvat.</skos:definition>
      <skos:prefLabel xml:lang="fr">Média : vidéo</skos:prefLabel>
      <skos:definition xml:lang="fr">Images en mouvement. Peut inclure des films, de l'animation, des enregistrements digitaux, des visualisations de simulations, des émissions de télévision enregistrées, des videos de diagnostic / médicales comme des échographies, etc. Peut être muet ou peut inclure du son synchronisé.</skos:definition>
      <skos:prefLabel xml:lang="fr">Média : vidéo</skos:prefLabel>
      <skos:definition xml:lang="fr">Images en mouvement. Peut inclure des films, de l'animation, des enregistrements digitaux, des visualisations de simulations, des émissions de télévision enregistrées, des videos de diagnostic / médicales comme des échographies, etc. Peut être muet ou peut inclure du son synchronisé.</skos:definition>
      <skos:prefLabel xml:lang="it">Unità di supporto: video</skos:prefLabel>
      <skos:definition xml:lang="it">Immagini in movimento. Può includere film, animazioni, registrazioni digitali, output visivi prodotti da simulazioni, programmi televisivi registrati, video medico/diagnostici come ecografie, ecc. Può essere muto o può includere il suono sincronizzato.</skos:definition>
      <skos:prefLabel xml:lang="no">Medieenhet: video</skos:prefLabel>
      <skos:definition xml:lang="no">Bevegelige bilder. Kan inkludere filmer, animasjoner, digitale opptak, visuell produksjon fra simuleringer, innspilte fjernsynsprogram, diagnostiske/medisinske videor som ultralyd skanninger, etc. Kan være uten lyd eller med synkronisert lyd.</skos:definition>
      <skos:prefLabel xml:lang="pt">Unidade de Media: Vídeo</skos:prefLabel>
      <skos:definition xml:lang="pt">Imagens em movimento. Pode incluir filmes, animação, gravações digitais, output visual de simulações, programas televisivos gravados, vídeos médicos/de diagnóstico como ultrasons, etc. Podem ser silenciosos ou podem incluir som sincronizado.</skos:definition>
      <skos:prefLabel xml:lang="sr">Medijski zapis: Video</skos:prefLabel>
      <skos:definition xml:lang="sr">Pokretne slike. Može da uključuje filmove, animacije, digitalne snimke, vizuelni autput simulacija, snimljenih televizijskih programa, dijagnostički / medicinski video snimak poput ultrazvučnog skeniranja, itd. Može, ali ne mora da uključuje sinhronizovani zvuk.</skos:definition>
      <skos:prefLabel xml:lang="sv">Mediaenhet: Video</skos:prefLabel>
      <skos:definition xml:lang="sv">Rörliga bilder. Det kan inkludera filmer, animationer, digitala inspelningar, visuella utdata från simuleringar, inspelade tv-program, diagnostiska/medicinska videor som ultraljudsundersökningar osv. De kan vara utan ljud eller med synkroniserat ljud.</skos:definition>
      <skos:broader rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e900"/>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e1298">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/"/>
      <skos:notation>Group</skos:notation>
      <skos:prefLabel xml:lang="en">Group</skos:prefLabel>
      <skos:definition xml:lang="en">Two or more individuals assembled together or having some unifying relationship or characteristic. In surveys, the questions are mainly focused on the group and the attitudes/experiences of the group, rather than on those of the individual respondents. If questions cover both the group and the individual, use  both as analysis units.</skos:definition>
      <skos:prefLabel xml:lang="da">Gruppe</skos:prefLabel>
      <skos:definition xml:lang="da">To eller flere individer samlet sammen eller, eller som har et forenende forhold eller karakteristik. I undersøgelser er spørgsmålene hovedsageligt fokuseret på gruppen og holdningerne/oplevelserne i gruppen snarere end de individuelle respondenters. Hvis spørgsmål dækker både gruppen og den individuelle, brug begge som analyseenheder.</skos:definition>
      <skos:prefLabel xml:lang="fi">Ryhmä</skos:prefLabel>
      <skos:definition xml:lang="fi">Kaksi tai useampia henkilöitä, jotka ovat joko kokoontuneet yhteen tai joita yhdistää jokin muu tekijä. Mikäli kyselytutkimus, kysymykset koskevat pääasiassa itse ryhmää, ryhmän kokemuksia ja asenteita ryhmää kohtaan, ei henkilöä itseään. Mikäli kysymykset koskevat sekä ryhmää että henkilöä, käytä havaintoyksikkönä molempia.</skos:definition>
      <skos:prefLabel xml:lang="fi">Ryhmä</skos:prefLabel>
      <skos:definition xml:lang="fi">Kaksi tai useampia henkilöitä, jotka ovat joko kokoontuneet yhteen tai joita yhdistää jokin muu tekijä. Mikäli kyselytutkimus, kysymykset koskevat pääasiassa itse ryhmää, ryhmän kokemuksia ja asenteita ryhmää kohtaan, ei henkilöä itseään. Mikäli kysymykset koskevat sekä ryhmää että henkilöä, käytä havaintoyksikkönä molempia.</skos:definition>
      <skos:prefLabel xml:lang="fr">Groupe</skos:prefLabel>
      <skos:definition xml:lang="fr">Deux ou plus individus réunis ou ayant une relation ou une caractéristique unificatrice. Dans les enquêtes, les questions sont principalement axées sur le groupe et les attitudes / expériences du groupe, plutôt que sur celles des répondants individuels. Si des questions couvrent à la fois le groupe et l'individu, utilisez les deux comme unité d'analyse.</skos:definition>
      <skos:prefLabel xml:lang="fr">Groupe</skos:prefLabel>
      <skos:definition xml:lang="fr">Deux ou plus individus réunis ou ayant une relation ou une caractéristique unificatrice. Dans les enquêtes, les questions sont principalement axées sur le groupe et les attitudes / expériences du groupe, plutôt que sur celles des répondants individuels. Si des questions couvrent à la fois le groupe et l'individu, utilisez les deux comme unité d'analyse.</skos:definition>
      <skos:prefLabel xml:lang="it">Gruppo</skos:prefLabel>
      <skos:definition xml:lang="it">Due o più individui riuniti oppure che possiedono una qualsiasi relazione o caratteristica unificante. Nelle indagini le domande si concentrano principalmente sul gruppo e sugli atteggiamenti/esperienze del gruppo, piuttosto che su quelle dei singoli intervistati. Se le domande riguardano sia il gruppo che l'individuo, utilizzare entrambi come unità di analisi.</skos:definition>
      <skos:prefLabel xml:lang="no">Gruppe</skos:prefLabel>
      <skos:definition xml:lang="no">To eller flere personer som samles eller som har et forhold som forener dem. I spørreundersøkelser er spørsmålene hovedsaklig rettet mot gruppen og holdningen/opplevelsene til gruppen i steden for de individuelle respondentene. Hvis spørsmål angår både gruppen og individene, bruk begge som analyseenhet.</skos:definition>
      <skos:prefLabel xml:lang="pt">Grupo</skos:prefLabel>
      <skos:definition xml:lang="pt">Dois ou mais indivíduos que se assemelham ou que têm uma relação ou característica unificadora. Nos inquéritos, as questões são principalmente focadas no grupo e nas atitudes e experiências do grupo, e não tanto nos indivíduos entrevistados. Se as questões abrangem o indivíduo e o grupo, usar ambos como unidades de análise.</skos:definition>
      <skos:prefLabel xml:lang="sr">Grupa</skos:prefLabel>
      <skos:definition xml:lang="sr">Dvoje ili više pojedinaca koji su okupljeni ili objedinjeni nekim odnosom ili karakteristikom(ama). U anketama, pitanja su uglavnom usmereni na grupu i stavove / iskustva grupe, a ne pojedinačnih ispitanika. Ako pitanja pokrivaju i grupu i pojedince, koristite oba kao jedinice za analizu.</skos:definition>
      <skos:prefLabel xml:lang="sv">Grupp</skos:prefLabel>
      <skos:definition xml:lang="sv">Två eller fler personer som samlats eller som har någon form av relation eller egenskap som förenar dem. I surveyundersökningar fokuserar frågorna framför allt på gruppen och gruppens attityder/erfarenheter, snarare än på de enskilda respondenterna. Om frågorna gäller både gruppen och individen, använd båda som analysenheter.</skos:definition>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e1377">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/"/>
      <skos:notation>Object</skos:notation>
      <skos:prefLabel xml:lang="en">Object</skos:prefLabel>
      <skos:definition xml:lang="en">Anything material, but inanimate, that has an independent existence and may be perceived by the senses. Examples: objects of art (paintings, sculptures, etc.) or weapons, or vehicles, etc.</skos:definition>
      <skos:prefLabel xml:lang="da">Objekt</skos:prefLabel>
      <skos:definition xml:lang="da">Ethvert livløst materiale, der har en uafhængig eksistens, og som kan opfattes af sanserne. Eksempler: kunstobjekter (malerier, skulpturer etc.) eller våben eller køretøjer etc.</skos:definition>
      <skos:prefLabel xml:lang="fi">Eloton objekti/esine</skos:prefLabel>
      <skos:definition xml:lang="fi">Mikä tahansa konkreettinen, eloton esine/kohde, joka voidaan aistein havaita. Esimerkiksi taideteokset (veistokset, maalaukset), aseet, ajoneuvot jne.</skos:definition>
      <skos:prefLabel xml:lang="fi">Eloton objekti/esine</skos:prefLabel>
      <skos:definition xml:lang="fi">Mikä tahansa konkreettinen, eloton esine/kohde, joka voidaan aistein havaita. Esimerkiksi taideteokset (veistokset, maalaukset), aseet, ajoneuvot jne.</skos:definition>
      <skos:prefLabel xml:lang="fr">Objet</skos:prefLabel>
      <skos:definition xml:lang="fr">Tout ce qui est matériel, mais inanimé, qui a une existence indépendante et peut être perçu par les sens. Exemples : objets d'art (peintures, sculptures, etc.) ou armes, ou véhicules, etc.</skos:definition>
      <skos:prefLabel xml:lang="fr">Objet</skos:prefLabel>
      <skos:definition xml:lang="fr">Tout ce qui est matériel, mais inanimé, qui a une existence indépendante et peut être perçu par les sens. Exemples : objets d'art (peintures, sculptures, etc.) ou armes, ou véhicules, etc.</skos:definition>
      <skos:prefLabel xml:lang="it">Oggetto</skos:prefLabel>
      <skos:definition xml:lang="it">Tutto ciò che è materiale, inanimato, che ha un'esistenza indipendente e che può essere percepito attraverso i sensi. Esempi: oggetti d'arte (dipinti, sculture, ecc.), armi, veicoli, ecc.</skos:definition>
      <skos:prefLabel xml:lang="no">Objekt</skos:prefLabel>
      <skos:definition xml:lang="no">Noe fysisk, men ikke levende som eksisterer som en uavhengig enhet og som kan oppfattes av sansene. For eksempel: kunst (malerier, skulpturer osv.), våpen, kjøretøy etc.</skos:definition>
      <skos:prefLabel xml:lang="pt">Objeto</skos:prefLabel>
      <skos:definition xml:lang="pt">Qualquer coisa material, mas inanimada, que tenha uma existência independente e possa ser percebida pelos sentidos. Exemplos: objetos de arte (pinturas, esculturas, etc.) ou armas, ou veículos, etc.</skos:definition>
      <skos:prefLabel xml:lang="sr">Predmet</skos:prefLabel>
      <skos:definition xml:lang="sr">Bilo šta materijalno, deo nežive prirode, pri čemu eguistira nezavisno i može biti opaženo čulima. Primeri: predmeti umetnosti (slike, skulpture, itd.) ili oružje ili vozila, zgrade itd.</skos:definition>
      <skos:prefLabel xml:lang="sv">Föremål</skos:prefLabel>
      <skos:definition xml:lang="sv">Något fysiskt men icke levande som existerar som en oberoende enhet och kan uppfattas av sinnena. Exempel: konstföremål (tavlor, skulpturer, o.dyl.), vapen, fordon m.m.</skos:definition>
   </rdf:Description>
   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e1455">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/"/>
      <skos:notation>Other</skos:notation>
      <skos:prefLabel xml:lang="en">Other</skos:prefLabel>
      <skos:definition xml:lang="en">Use if the unit of analysis is known, but not found in the list.</skos:definition>
      <skos:prefLabel xml:lang="da">Andet</skos:prefLabel>
      <skos:definition xml:lang="da">Brug denne, hvis analyseenheden er kendt, men ikke fremgår af listen.</skos:definition>
      <skos:prefLabel xml:lang="fi">Muu havainto/aineistoyksikkötyyppi</skos:prefLabel>
      <skos:definition xml:lang="fi">Käytä mikäli havaintoyksikkö on tiedossa, mutta ei löydy sanastosta.</skos:definition>
      <skos:prefLabel xml:lang="fi">Muu havainto/aineistoyksikkötyyppi</skos:prefLabel>
      <skos:definition xml:lang="fi">Käytä mikäli havaintoyksikkö on tiedossa, mutta ei löydy sanastosta.</skos:definition>
      <skos:prefLabel xml:lang="fr">Autre</skos:prefLabel>
      <skos:definition xml:lang="fr">Utilisez si l'unité d'analyse est connue, mais pas présente dans la liste.</skos:definition>
      <skos:prefLabel xml:lang="fr">Autre</skos:prefLabel>
      <skos:definition xml:lang="fr">Utilisez si l'unité d'analyse est connue, mais pas présente dans la liste.</skos:definition>
      <skos:prefLabel xml:lang="it">Altro</skos:prefLabel>
      <skos:definition xml:lang="it">Utilizzare se l'unità di analisi è nota, ma non riconducibile a nessun termine presente nell'elenco.</skos:definition>
      <skos:prefLabel xml:lang="no">Annet</skos:prefLabel>
      <skos:definition xml:lang="no">Benytt hvis analyseenheten er kjent, men ikke finnes i listen.</skos:definition>
      <skos:prefLabel xml:lang="pt">Outro </skos:prefLabel>
      <skos:definition xml:lang="pt">Usar se a unidade de análise for conhecida, mas não se encontrar na lista.</skos:definition>
      <skos:prefLabel xml:lang="sr">Ostalo</skos:prefLabel>
      <skos:definition xml:lang="sr">Koristite ako je jedinica analize poznata, ali nije na listi.</skos:definition>
      <skos:prefLabel xml:lang="sv">Övrigt</skos:prefLabel>
      <skos:definition xml:lang="sv">Använd om analysenheten är känd men saknas i listan.</skos:definition>
   </rdf:Description>
</rdf:RDF>
