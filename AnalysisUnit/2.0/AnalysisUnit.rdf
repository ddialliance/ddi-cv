   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/d56e116">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      <skos:inScheme rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/AnalysisUnit/2.0/"/>
      <skos:notation>Individual</skos:notation>
      <skos:prefLabel xml:lang="en">Individual</skos:prefLabel>
      <skos:definition xml:lang="en">Any individual person.</skos:definition>
      <skos:prefLabel xml:lang="da">Individ</skos:prefLabel>
      <skos:definition xml:lang="da">Enhver individuel person.</skos:definition>
      <skos:prefLabel xml:lang="fi">Henkilö</skos:prefLabel>
      <skos:definition xml:lang="fi">Tutkimuksen tarkastelukohteena ovat yksittäiset henkilöt</skos:definition>
      <skos:prefLabel xml:lang="fi">Henkilö</skos:prefLabel>
      <skos:definition xml:lang="fi">Tutkimuksen tarkastelukohteena ovat yksittäiset henkilöt</skos:definition>
      <skos:prefLabel xml:lang="fr">Individu</skos:prefLabel>
      <skos:definition xml:lang="fr">Toute personne individuelle.</skos:definition>
      <skos:prefLabel xml:lang="fr">Individu</skos:prefLabel>
      <skos:definition xml:lang="fr">Toute personne individuelle.</skos:definition>
      <skos:prefLabel xml:lang="it">Individuo</skos:prefLabel>
      <skos:definition xml:lang="it">Qualsiasi persona.</skos:definition>
      <skos:prefLabel xml:lang="no">Individ</skos:prefLabel>
      <skos:definition xml:lang="no">Enhver individuell person.</skos:definition>
      <skos:prefLabel xml:lang="pt">Indivíduo</skos:prefLabel>
      <skos:definition xml:lang="pt">Qualquer sujeito individual.</skos:definition>
      <skos:prefLabel xml:lang="sr">Pojedinac</skos:prefLabel>
      <skos:definition xml:lang="sr">Bilo koja osoba.</skos:definition>
      <skos:prefLabel xml:lang="sv">Individ</skos:prefLabel>
      <skos:definition xml:lang="sv">En enskild person.</skos:definition>
