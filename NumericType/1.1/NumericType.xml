   <ddi:Fragment>
      <l:Category>
         <r:Agency>int.ddi.cv</r:Agency>
         <r:ID>NumericType-Integer-Category</r:ID>
         <r:Version>1.1</r:Version>
         <l:CategoryName>Integer</l:CategoryName>
         <r:Label>
            <r:Content xml:lang="en">Integer</r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">Whole numbers,  the infinite set of integers, no minimum or maximum value.  An integer datatype corresponding to the W3C XML Schema's xs:integer datatype.</r:Content>
         </r:Description>
      </l:Category>
   </ddi:Fragment>
   <ddi:Fragment>
      <l:Category>
         <r:Agency>int.ddi.cv</r:Agency>
         <r:ID>NumericType-Int-Category</r:ID>
         <r:Version>1.1</r:Version>
         <l:CategoryName>Int</l:CategoryName>
         <r:Label>
            <r:Content xml:lang="en">Int </r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">Whole numbers in the range -2147483648 .. 2147483647. An integer datatype corresponding to the W3C XML Schema's xs:int datatype.</r:Content>
         </r:Description>
      </l:Category>
   </ddi:Fragment>
