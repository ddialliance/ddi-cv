         <r:Label>
            <r:Content xml:lang="en">Long</r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">Whole numbers in the range -9223372036854775808 .. 9223372036854775807. A numeric datatype corresponding to the W3C XML Schema's xs:long datatype.</r:Content>
         </r:Description>
      </l:Category>
   </ddi:Fragment>
   <ddi:Fragment>
