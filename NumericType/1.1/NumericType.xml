         <r:Version>1.1</r:Version>
         <l:CategoryName>Other</l:CategoryName>
         <r:Label>
            <r:Content xml:lang="en">Other</r:Content>
         </r:Label>
         <r:Description>
            <r:Content xml:lang="en">Use if the numeric type is known, but not found in the list.</r:Content>
         </r:Description>
      </l:Category>
