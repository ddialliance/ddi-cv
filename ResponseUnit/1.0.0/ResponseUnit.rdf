   <rdf:Description rdf:about="http://rdf-vocabulary.ddialliance.org/cv/ResponseUnit/1.0.0/">
      <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#ConceptScheme"/>
      <dcterms:isVersionOf rdf:resource="http://rdf-vocabulary.ddialliance.org/cv/ResponseUnit/"/>
      <skos:notation xml:lang="en">ResponseUnit</skos:notation>
      <dcterms:title xml:lang="en">Response Unit</dcterms:title>
      <dcterms:description xml:lang="en">Indicates the entity that provided the information carried by the variable.</dcterms:description>
      <skos:prefLabel xml:lang="en">Response Unit</skos:prefLabel>
      <dcterms:title xml:lang="no">Svarenhet</dcterms:title>
      <dcterms:description xml:lang="no">Indikerer enheten som har gitt informasjonen som er i variabelen.</dcterms:description>
      <skos:prefLabel xml:lang="no">Svarenhet</skos:prefLabel>
      <owl:versionInfo>1.0.0</owl:versionInfo>
      <dcterms:license rdf:resource="http://creativecommons.org/licenses/by/4.0/"/>
      <dcterms:rights>Copyright &#xa9;DDI Alliance 2020</dcterms:rights>
